<div class="col-md-12">
  <table class="table table-striped table-condensed">
    <thead>
      <tr>
        <th>{{ "Name"|trans }}</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      {% for vocabulary in vocabularies %}
        <tr>
          <td>
            {{ vocabulary.name|e }}
          </td>
          <td>
            {{ ucms_actions(vocabulary) }}
          </td>
        {% else %}
          <tr>
            <td colspan="2">
              <p>{{ "There is no vocabularies you have access to"|trans }}</p>
            </td>
          </tr>
        {% endfor %}
      </tr>
    </tbody>
  </table>
</div>